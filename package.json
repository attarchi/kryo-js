{
  "name": "kryo-js",
  "version": "0.0.1",
  "description": "Port from https://github.com/EsotericSoftware/kryo",
  "main": "index.ts",
  "scripts": {
    "start": "npm run build && node ./dist/",
    "build": "npx tsc",
    "build:watch": "npx tsc --watch",
    "lint": "npm run eslint && npm run prettier:check",
    "eslint": "eslint --report-unused-disable-directives .",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/attarchi/kryo-js.git"
  },
  "keywords": [
    "kryo"
  ],
  "author": "Ryan @archi",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/attarchi/kryo-js/issues"
  },
  "homepage": "https://github.com/attarchi/kryo-js#readme",
  "devDependencies": {
    "@types/node": "^20.8.7",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^5.0.1",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.4",
    "typescript-eslint": "^0.0.1-alpha.0"
  }
}
